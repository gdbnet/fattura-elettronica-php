{
    "name": "gdbnet/fattura-elettronica-php",
    "description": "PHP class for italian electronic invoice xml",
    "keywords": [
        "php",
        "fattura",
        "xml"
    ],
    "version": "1.0.4",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php":">=7.1",
        "ext-json":"*",
        "ext-dom":"*",
        "ext-libxml":"*",
        "symfony/serializer":"^2.8.50",
        "ext-xsl":"*"
    },
    "require-dev": {
        "phpunit/phpunit":"^7"
    },
    "autoload": {
        "psr-4": {
            "Gdbnet\\FatturaElettronica\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gdbnet\\FatturaElettronica\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
