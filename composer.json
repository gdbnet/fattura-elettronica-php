{
    "name": "gdbnet/fattura-elettronica-php",
    "description": "PHP class for italian electronic invoice xml",
    "keywords": [
        "php",
        "fattura",
        "xml"
    ],
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "symfony/serializer": "^4.2",
        "ext-xsl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Gdbnet\\FatturaElettronica\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gdbnet\\FatturaElettronica\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
